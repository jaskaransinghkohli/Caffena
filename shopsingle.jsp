		 var pr_name='${PRODUCT.product_name}';
		 var unit=jQuery("#unit-sel").val();
		 
		 $.ajax({
	  		url:"check-availability-of-product",
	  		traditional: true,
	  		type: 'POST',
	  		data:{product_name: pr_name,
				unit:unit
	  		},
	  		

	  		 success:function(response)
	  		 {
	                   if(response.includes('FOUND')){
	                	   var str=response.substring(5);
	                	   
	                	   var str1=str.split(",");
	                	   var price=str1[0];
	                	   var stocks=str1[1];
	                	   
	                	   stocks=parseInt(stocks);
	                	   
	                	   global_price=stocks;
	                	  price=parseInt(price);
	                	  var mrp=price+((price/100)*10);
	                	  var dis= price*0.10;
	                	 /*  price= <fmt:formatNumber type="number" value="${price}" />
	                	  mrp=<fmt:formatNumber type="number" value="${mrp}" />
	                	  dis=<fmt:formatNumber type="number" value="${dis}" /> */
	                	 	 mrp= mrp.toLocaleString(undefined, { minimumFractionDigits: 0 });
	                	  	price=price.toLocaleString(undefined, { minimumFractionDigits: 0 });
	                	  	dis=dis.toLocaleString(undefined, { minimumFractionDigits: 0 });
	                	  
	                	  $("#stockConformation").html("Maximum Available item is"+stocks);	
	                	  $("#Stock").show();
	                	   $("#outOfStock").hide();
	                	   $("#amt").html(price);
	                	   $("#dis").html(dis);
	                	   $("#mrp").html(mrp);
	                	   $("#stockConformation").show();
	                	   $("#price").css("display", "none")
	                	   $("input:submit").prop("disabled", false);
	                	   
	                	   
	                   }
	                   else{
	                	   $("#Stock").hide();
	                	   $("#stockConformation").hide();
	                	   $("#outOfStock").show();
	                	   $("input:submit").prop("disabled", true);
	                   }
		   		}
	  		}); 
		}
	
